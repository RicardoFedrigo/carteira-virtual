{
  "name": "carteira-virtual",
  "version": "1.0.0",
  "description": "Carteira virtual API",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "nodemon --watch 'src/' --exec 'ts-node src/index.ts' -e ts",
    "start": "ts-node src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:run": "ts-node ./node_modules/.bin/typeorm migrations:run",
    "typeorm:clear": "ts-node ./node_modules/.bin/typeorm schema:drop",
    "test": "jest",
    "build": "rimraf build && tsc -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RicardoFedrigo/carteira-virtual.git"
  },
  "keywords": [
    "API",
    "Carteira-virtual"
  ],
  "author": "Ricardo Fedrigo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RicardoFedrigo/carteira-virtual/issues"
  },
  "homepage": "https://github.com/RicardoFedrigo/carteira-virtual#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/chai": "^4.2.18",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^8.10.66",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-core": "6.14.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.3.4",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^5.5.3",
    "prettier": "^2.0.5",
